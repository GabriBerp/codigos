if place_meeting(x,y,oGabiBoss2){
	for(var i = 0; i<instance_number(oTeleporter);i++) {
		var inst = instance_find(oTeleporter,i);
		if inst.identi = alvo {
		oGabiBoss2.x = inst.xtp;
		oGabiBoss2.y = inst.ytp;
		if identi = 6 {
		alvo = choose(2,5,1);
		}
		if identi = 3 {
		alvo = choose(0,4);
		}
		}
	}
}

